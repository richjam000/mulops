{
   "catalog": "teststatus",
   "author": "Richard.Jamieson_scsuk.net",
   "catalog-description": "Used to track mulops testing status",
   "catalog-version": "3.1",
   "mulops-commands": {
      "teststatus": {
         "description":         "Mulops Test Status for each Operating System version",
         "variations": [
            { "oscommand":      "echo Tested",
              "ostype":         [ "eq","unix_linux_ubuntu"],
              "osversion":      [ "eq","14.04"]
            },
            { "oscommand":      "echo Tested",
              "ostype":         [ "eq","unix_linux_centos"],
              "osversion":      [ "=~","^7"]
            },
            { "oscommand":      "echo Tested",
              "ostype":         [ "eq","unix_linux_centos"],
              "osversion":      [ "=~","^6"]
            },
            { "oscommand":      "echo Tested",
              "ostype":         [ "eq","unix_solaris"],
              "osversion":      [ "eq","5.10"]
            },
            { "oscommand":      "echo Tested",
              "ostype":         [ "eq","unix_solaris"],
              "osversion":      [ "=~","11.3"]
            },
            { "oscommand":      "echo Tested",
              "ostype":         [ "=~","^Microsoft Windows 10"],
              "osversion":      [ "=~","10"]
            },
            { "oscommand":      "echo Not Tested - please test and update Richard.Jamieson@scsuk.net"
            }
         ]
      }
   }
}
